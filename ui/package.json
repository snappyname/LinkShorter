		"@vendure/ngx-translate-extract": "^10.1.2",
		"angular-eslint": "21.0.1",
		"eslint": "^9.39.1",
		"glob": "^13.0.0",
		"globals": "^16.5.0",
		"i18next-cli": "^1.37.1",
		"i18next-parser": "^9.0.2",
		"i18next-toolkit": "^1.1.0",
		"jasmine-core": "~5.9.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
