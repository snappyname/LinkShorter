{
	"name": "template-web-auth",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"lint": "ng lint",
		"deploy": "npm run lint && npm run build",
		"translate": "npx i18next-cli extract",
		"test": "ng test"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"printWidth": 120,
		"singleQuote": true,
		"overrides": [
			{
				"files": "*.html",
				"options": {
					"parser": "angular"
				}
			},
			{
				"files": [
					"*.less",
					"*.css",
					"*.scss",
					"*.sass"
				],
				"options": {
					"parser": "css"
				}
			}
		]
	},
	"private": true,
	"dependencies": {
		"@angular/cdk": "^21.0.0",
		"@angular/common": "^21.1.0",
		"@angular/compiler": "^21.1.0",
		"@angular/core": "^21.1.0",
		"@angular/forms": "^21.1.0",
		"@angular/platform-browser": "^21.1.0",
		"@angular/router": "^21.1.0",
		"@microsoft/signalr": "^10.0.0",
		"@ngx-translate/core": "^17.0.0",
		"@ngx-translate/http-loader": "^17.0.0",
		"@ngxs/devtools-plugin": "21.0.0",
		"@ngxs/form-plugin": "21.0.0",
		"@ngxs/hmr-plugin": "21.0.0",
		"@ngxs/logger-plugin": "21.0.0",
		"@ngxs/router-plugin": "21.0.0",
		"@ngxs/storage-plugin": "21.0.0",
		"@ngxs/store": "^21.0.0",
		"@ngxs/websocket-plugin": "21.0.0",
		"@taiga-ui/addon-table": "^4.68.0",
		"@taiga-ui/core": "^4.67.0",
		"@taiga-ui/icons": "^4.67.0",
		"@taiga-ui/icons-fontawesome": "^4.67.0",
		"@taiga-ui/kit": "^4.67.0",
		"i18next": "^25.8.0",
		"install": "^0.13.0",
		"jwt-decode": "^4.0.0",
		"libphonenumber-js": "1.12.34",
		"npm": "^11.7.0",
		"rxjs": "~7.8.0",
