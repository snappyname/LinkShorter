{
  "ConnectionStrings": {
    "Database": "${DB_PASSWORD_LINE}"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Jwt": {
     "Key": "${JWT_KEY}"
  },
  "Google": {
    "ClientId": "", //from https://console.cloud.google.com/auth/clients
    "ClientSecret": "${OAUTH_GOOGLE_SECRET}", //from https://console.cloud.google.com/auth/clients
    "RedirectUrl": "http://localhost:4200/auth/google/callback", //from app settings on google app page
    "GrantType": "authorization_code",
    "RequestUrl": "https://oauth2.googleapis.com/token"
  },
  "Github": {
    "ClientId": "Ov23lioLfY67fwYvviS0", //from https://github.com/settings/developers 
    "ClientSecret": "${OAUTH_GITHUB_SECRET}",//from https://github.com/settings/developers 
    "RedirectUrl": "http://localhost:4200/auth/github/callback",
    "AppName": "TestApp"
  },
  "CORS": {
    "FrontendUrl": "${CORS_FRONTEND_URL}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
